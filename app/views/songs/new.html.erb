


<h1>Add a New Song</h1>

<%= render "shared/error_messages", model: @song %>

<%= form_for @song do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :duration %>
  <%= form.number_field :duration %>
  <br>
  <br>
  <%= form.submit "Add my Song!" %>
<% end %>
